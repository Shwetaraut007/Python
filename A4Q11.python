def generate_primes(n):
    for num in range(2, n + 1):
        is_prime = True
        for i in range(2, int(num**0.5) + 1):
            if num % i == 0:
                is_prime = False
                break
        if is_prime:
            yield num

n = int(input("Generate prime numbers up to: "))
print(f"Prime numbers up to {n}:")
for prime in generate_primes(n):
    print(prime, end=" ")
