class MyError(Exception):
    def __init__(self, message, extra_data):
        super().__init__(message)
        self.extra_data = extra_data

def oops():
    raise MyError("Something went wrong!", {"info": "Extra data here"})

def catcher():
    try:
        oops()
    except IndexError as e:
        print("Caught IndexError:", e)
    except MyError as e:
        print("Caught MyError:", e)
        print("Extra data:", e.extra_data)

catcher()
