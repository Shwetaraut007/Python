my_dict = {
    "name": "John",
    "age": 30,
    "city": "New York"
}

key_to_check = input("Enter the key to check: ")

if key_to_check in my_dict:
    print(f"Key '{key_to_check}' exists in the dictionary.")
else:
    print(f"Key '{key_to_check}' does NOT exist in the dictionary.")



